{
  "name": "@hyrious/npm-browser",
  "type": "module",
  "version": "0.1.0",
  "description": "Download and view npm package contents in the browser",
  "author": "hyrious <hyrious@outlook.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hyrious/npm-browser.git"
  },
  "keywords": [
    "npm",
    "browser"
  ],
  "bin": {
    "npm-browser": "dist/cli.js"
  },
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build -c vite.config.ssr.ts",
    "build:npm": "vite build -c vite.config.npm.ts && jiti scripts/build-cli.ts",
    "preview": "vite preview",
    "preview:npm": "node dist/cli.js"
  },
  "devDependencies": {
    "@hyrious/configs": "^0.1.3",
    "@iconify-json/carbon": "^1.2.8",
    "@iconify-json/codicon": "^1.2.15",
    "@rollup/plugin-commonjs": "^28.0.3",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@stylistic/eslint-plugin-ts": "^4.2.0",
    "@types/node": "^22.14.0",
    "@types/pako": "^2.0.3",
    "@vitejs/plugin-vue": "^5.2.3",
    "esbuild": "^0.25.2",
    "eslint": "^9.24.0",
    "eslint-plugin-vue": "^10.0.0",
    "find-my-way": "^9.3.0",
    "jiti": "^2.4.2",
    "pako": "^2.1.0",
    "rollup": "^4.39.0",
    "sade": "^1.8.1",
    "sanitize.css": "^13.0.0",
    "sirv": "^3.0.1",
    "structured-clone-es": "^1.0.0",
    "typescript-eslint": "^8.29.1",
    "unocss": "66.1.0-beta.10",
    "vite": "^6.2.5",
    "vue": "^3.5.13",
    "vue-eslint-parser": "^10.1.3"
  },
  "pnpm": {
    "neverBuiltDependencies": []
  }
}
